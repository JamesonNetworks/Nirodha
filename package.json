{
  "name": "nirohda",
  "version": "1.0.7",
  "description": "Website deployment software",
  "homepage": "http://www.jamesonnetworks.com",
  "author": "Brent Jameson <brent@jamesonnetworks.com>",
  "dependencies": {
    "async": "0.9.x",
    "commander": "2.3.x",
    "node-minify": "0.10.x",
    "colors": "0.6.x",
    "jslogging": "0.1.x",
    "underscore": "1.7.x",
    "watch": "0.11.x",
    "coveralls": "2.x.x"
  },
  "devDependencies": {
    "mocha": "1.17.x",
    "should": "3.1.x",
    "istanbul": "0.2.x",
    "jshint": "2.4.x",
    "mocha-lcov-reporter": "0.0.1"
  },
  "scripts": {
    "test": "mocha -R spec -u tdd --recursive test/",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -u tdd -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "engine": "node >= 0.10.x"
}
